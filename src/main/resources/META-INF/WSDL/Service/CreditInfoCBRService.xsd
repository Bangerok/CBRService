        <xs:complexType>
            <xs:sequence>
                <xs:element name="bicCodesXML" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="ServiceResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name= "typeResponse" type="xs:string"/>
                <xs:element name= "additionalInfo" type="xs:string"/>
                <xs:element name= "file" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>